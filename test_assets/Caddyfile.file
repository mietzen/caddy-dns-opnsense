{
	auto_https off
	dynamic_dns {
		provider opnsense {
			host {env.OPNSENSE_HOST}
			api_key {file./tmp/opnsense_api_key}
			api_secret_key {file./tmp/opnsense_api_secret_key}
			dns_service dnsmasq
			insecure true
			entry_description "Managed by Caddy Test"
		}
		domains {
			example.com test
		}
		ip_source static 192.168.42.23
		check_interval 1s
		ttl 1h
	}
}

http://test.example.com:8080 {
	respond "Caddy file placeholder test server"
}
